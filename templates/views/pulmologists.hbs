        // Michael


        document.getElementById('load-excel-button1').addEventListener('click', function () {
          const fileInput1 = document.getElementById('file-input1');
          const excelTable1 = document.getElementById('excel-table1');
      
          if (fileInput1.files.length === 0) {
            alert('Please select an Excel file first.');
            return;
          }
      
          const file1 = fileInput1.files[0];
          const reader1 = new FileReader();
      
          reader1.onload = function (e) {
            const data1 = new Uint8Array(e.target.result);
            const workbook1 = XLSX.read(data1, { type: 'array' });
      
            // Assuming the first sheet in the Excel file should be used
            const sheetName1 = workbook1.SheetNames[0];
            const sheet1 = workbook1.Sheets[sheetName1];
      
            const tableBody1 = excelTable1.querySelector('tbody');
            tableBody1.innerHTML = XLSX.utils.sheet_to_html(sheet1);
      
            // You can add additional styling or formatting here if needed
      
            fileInput1.value = ''; // Reset the file input
          };
      
          reader1.readAsArrayBuffer(file1);
        });


