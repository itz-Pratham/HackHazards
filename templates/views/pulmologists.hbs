      <script>
        function showSchedule1() {
          const schedule1 = document.getElementById('schedule1');
          if (schedule1.style.display === 'none' || schedule1.style.display === '') {
            schedule1.style.width = '40%';
            schedule1.style.display = 'grid';
            schedule1.style.marginLeft = '55%';
            schedule1.style.marginTop = '-600px';
          } else {
            schedule1.style.display = 'none';
          }
        }

